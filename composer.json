{
	"name": "regulus/identify",
	"description": "A Laravel 5 authentication/authorization package that adds roles, permissions, access levels, and user states. Allows simple or complex user access control implementation.",
	"license": "MIT",
	"authors": [
		{
			"name": "Cody Jassman",
			"email": "me@codyjassman.com"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"laravel/framework": ">=5.2.0",

		"regulus/solid-site": "0.6.*",
		"regulus/tetra-text": "0.5.*"
	},
	"autoload": {
		"classmap": [
			"src/migrations",
			"src/seeds"
		],
		"psr-4": {
			"Regulus\\Identify\\": "src/",
			"Regulus\\Identify\\Commands\\": "src/commands/",
			"Regulus\\Identify\\Libraries\\": "src/libraries/",
			"Regulus\\Identify\\Middleware\\": "src/middleware/",
			"Regulus\\Identify\\Models\\": "src/models/"
		}
	}
}
